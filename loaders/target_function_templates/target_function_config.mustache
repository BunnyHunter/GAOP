<template>
    <div class="config-root">
    {{#items}}
        {{{html}}}
    {{/items}}
    </div>
</template>

<script>
export const targetFunctionName = "{{targetFunctionName}}"
export default {
    name: "{{targetFunctionName}}.tfcfg",
    props: ["value"],
    methods: {
        handleChangeValue(name, value) {
            let temp = Object.assign({}, this.value.{{targetFunctionName}})
            temp[name] = value
            this.$emit("input", {
                "{{targetFunctionName}}": temp
            })
        },
    },
    getDefaultConfig() {
        const defaultConfig = {
            "{{targetFunctionName}}": {
            {{#items}}
                {{name}}: {{default}},
            {{/items}}
            }
        }
        return defaultConfig
    }
}
</script>

